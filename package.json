{
  "name": "shubhamgulati.com",
  "version": "0.1.0",
  "type": "module",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "dev": "next dev & pnpm watch",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "typecheck": "tsc --noEmit",
    "lint": "next lint --cache-location 'node_modules/.cache/.eslintcache'",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --check . --cache --cache-location='node_modules/.cache/.prettiercache' --ignore-path='.gitignore' --ignore-path='.prettierignore'",
    "format:fix": "pnpm run format --write",
    "serve": "pnpm dlx serve@latest out",
    "deploy": "gh-pages --dist out --nojekyll",
    "watch": "jiti ./src/scripts/watcher.ts",
    "clean": "git clean -xdf node_modules dist out .next",
    "prepare": "husky"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.5",
    "@radix-ui/react-context": "^1.1.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^3.1.1",
    "cmdk": "^1.0.0",
    "next": "^14.2.5",
    "next-themes": "1.0.0-beta.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4",
    "valibot": "^0.36.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "@next/bundle-analyzer": "^14.2.5",
    "@types/eslint": "^8.56.10",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "chokidar": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "gh-pages": "^6.1.1",
    "glob": "^11.0.0",
    "husky": "^9.0.11",
    "jiti": "^1.21.6",
    "prettier": "^3.3.2",
    "shikiji": "^0.10.2",
    "typescript": "^5.5.3",
    "ws": "^8.18.0"
  },
  "packageManager": "pnpm@9.4.0",
  "pnpm": {
    "patchedDependencies": {
      "@radix-ui/themes@3.1.1": "patches/@radix-ui__themes@3.1.1.patch"
    }
  }
}
